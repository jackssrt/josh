{
	"name": "splatsquad-bot",
	"type": "module",
	"version": "1.0.0",
	"exports": "./index.js",
	"engines": {
		"node": "^12.20.0 || ^14.13.1 || >=16.0.0"
	},
	"browser": {
		"fs": false,
		"path": false,
		"os": false
	},
	"dependencies": {
		"@discordjs/opus": "^0.9.0",
		"@discordjs/voice": "^0.16.0",
		"axios": "^1.4.0",
		"consola": "^3.1.0",
		"discord.js": "^14.11.0",
		"dotenv": "^16.1.4",
		"ffmpeg-static": "^5.1.0",
		"google-tts-api": "^2.0.2",
		"js-levenshtein": "^1.1.6",
		"ngraph.forcelayout": "^3.3.1",
		"ngraph.graph": "^20.0.1",
		"pm2": "^5.3.0",
		"sharp": "^0.32.1",
		"sodium-native": "^4.0.3"
	},
	"devDependencies": {
		"@types/d3": "^7.4.0",
		"@types/js-levenshtein": "^1.1.1",
		"@types/node": "^20.2.5",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"@vitest/coverage-v8": "^0.32.0",
		"eslint": "^8.42.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-prettier": "^4.2.1",
		"nodemon": "^2.0.22",
		"prettier": "^2.8.8",
		"ts-node": "^10.9.1",
		"typescript": "^5.1.3",
		"vitest": "^0.32.2"
	},
	"scripts": {
		"dev": "nodemon",
		"deploy": "ts-node-esm src/deploy.ts",
		"lint": "eslint ./src/**/*.ts",
		"build": "tsc",
		"start": "pm2 start ./build/index.js && pm2 log",
		"stop": "pm2 stop ./build/index.js",
		"check": "tsc --noEmit && eslint ./src/**/*.ts && vitest run",
		"test": "vitest",
		"debug": "tsc --sourceMap && node ./build/index.js"
	}
}